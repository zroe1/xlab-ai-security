    "\n",
    "        attackers.append(attacker)\n",
    "\n",
    "    # main TAP loop\n",
    "    for d in range(depth):\n",
    "        print(f\"Depth {d + 1}\")\n",
    "\n",
    "        # prune: keep only top 'width' attackers\n",
    "        attackers.sort(key=lambda x: x.score, reverse=True)\n",
    "        attackers = attackers[:width]\n",
    "\n",
